<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="egovframework.ddan.mapper.TempMapper">


	<insert id="insert">
	    insert into temp (latitude, longitude, date, time, andoid_id)
	    	values (#{latitude}, #{longitude}, CURRENT_DATE, CURRENT_TIME, #{andoid_id})
	</insert>
	
	<select id="getLocaList" resultType="egovframework.ddan.vo.LocationVo">
		
		select * from temp
		
	</select>
	
	<delete id="deleteTemt">
		
		delete from temp			
			
	</delete>

</mapper>